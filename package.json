{
  "devWatch": {
    "cmd": "dev",
    "runs": [
      {
        "type": "typescript",
        "browserify": false,
        "configFile": "src/tsconfig.json",
        "declarations": true
      }
    ]
  },
  "engines": {
    "node": ">= 4"
  },
  "main": "./lib/index",
  "keywords": [
    "webdav-server",
    "webdav",
    "server",
    "virtual"
  ],
  "scripts": {
    "prepare": "yarn build",
    "dev": "cd src && tsc --declaration -w",
    "build": "cd src && tsc --declaration",
    "lint": "tslint --project tslint.json src/**/v2/**/*.ts",
    "test": "yarn build-test && yarn start-test",
    "start-test": "node test/v2/root.js && node test/v1/root.js",
    "build-test": "cd test/v2/tests.ts && tsc"
  },
  "name": "webdav-server",
  "version": "2.7.0",
  "author": "Adrien Castex <adrien.castex@protonmail.com>",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/Smithsonian/npm-WebDAV-Server.git"
  },
  "description": "WebDAV Server",
  "dependencies": {
    "mime-types": "^2.1.34",
    "tmp-promise": "^3.0.3",
    "winston": "^3.3.3",
    "xml-js-builder": "^1.0.3"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.12",
    "@types/request": "2.48.7",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "request": "^2.88.2",
    "safe-regex": "^2.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "webdav-fs": "https://github.com/Smithsonian/webdav-fs.git",
    "xml-js": "^1.6.11"
  },
  "types": "./lib/index.d.ts"
}
